pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello world'
                sleep 3
            }
        }
         stage('build') {
            steps {
                echo 'Hello build'
                sleep 3
            }
        }
         stage('deploy') {
            steps {
                echo 'Hello deploy'
                sleep 3
            }
        }
         stage('test') {
            steps {
                echo 'Hello test'
                sleep 3
            }
        }
        
    }
}

